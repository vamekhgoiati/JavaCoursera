package guimodule;

import processing.core.PApplet;

public class MyDisplay extends PApplet {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Override
	public void setup() {
		size(400, 400);
		background(200, 200, 200);
		fill(255, 255, 0);
	}

	@Override
	public void draw() {
		ellipse(200, 200, 300, 300);
	}
}
